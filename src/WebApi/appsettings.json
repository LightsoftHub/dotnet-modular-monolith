{
  "Environment": "UAT",
  "ConnectionStrings": {
    "UserConnection": "Server=(localdb)\\MSSQLLocalDB;Database=SeedDb;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Hangfire": "Warning",
      "Microsoft": "Warning",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Hangfire": "Warning",
        "Microsoft": "Error",
        "System": "Information",
        "Microsoft.Hosting.Lifetime": "Information",
        "Microsoft.EntityFrameworkCore.Database.Command": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "FileAsync"
      }
    ]
  },
  "RequestLogging": {
    "Enable": true,
    "IncludeRequest": false,
    "IncludeResponse": false,
    "ExcludePaths": [ "hc" ]
  },
  "AllowAnonymous": true,
  "Swagger": {
    "Enable": true,
    "Title": "eShop API",
    "AuthMode": "JWT"
  },
  "JWT": {
    "Issuer": "https://localhost",
    "SecretKey": "3CC79718-C525-4F1A-AFCF-E9F3722C6008"
  },
  "DomainName": null
}
